<%@ page contentType="text/html;charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<c:set var="ctx" value="${pageContext.request.contextPath}" />

<html>
<head>
<title>用户管理</title>
</head>

<body>
	<form class="form-horizontal"  id="inputForm" action="#" method="post" >
		<div class="control-group">
			<label for="beEvaluatedName" class="control-label">被评价者:</label>
			<div class="controls">
				<input type="text" id="beEvaluatedName" name="beEvaluatedName"
					class="input-large " value="${p.beEvaluatedName}" readonly />
			</div>
		</div>
		<div class="control-group">
			<label for="departmentName" class="control-label">部门:</label>
			<div class="controls">
				<input type="text" id="departmentName" name="departmentName"
					class="input-large" value="${p.departmentName}" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="id" class="control-label">绩效总评ID:</label>
			<div class="controls">
				<input type="text" id="id" name="id" class="input-large"
					value="${p.id}" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="efficiencyScore" class="control-label">效能:</label>
			<div class="controls">
				<input type="text" id="efficiencyScore" name="efficiencyScore"
					class="input-large" value="${p.efficiencyScore}分" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="specialtyScore" class="control-label">专业:</label>
			<div class="controls">
				<input type="text" id="specialtyScore" name="specialtyScore"
					class="input-large" value="${p.specialtyScore}分" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="leaderAssessmentScore" class="control-label">上级评价:</label>
			<div class="controls">
				<input type="text" id="leaderAssessmentScore"
					name="leaderAssessmentScore" class="input-large"
					value="${p.leaderAssessmentScore}分" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="satisfactionScore" class="control-label">客户满意:</label>
			<div class="controls">
				<input type="text" id="satisfactionScore" name="satisfactionScore"
					class="input-large" value="${p.satisfactionScore}分" readonly/>
			</div>
		</div>
		<div class="control-group">
			<label for="totalScore" class="control-label">总分:</label>
			<div class="controls">
				<input type="text" id="totalScore" name="totalScore"
					class="input-large" value="${p.totalScore}分" readonly />
			</div>
		</div>
		<div class="control-group">
			<label for="totalGrade" class="control-label">评级:</label>
			<div class="controls">
				<input type="text" id="totalGrade" name="totalGrade"
					class="input-large" value="${p.totalGrade}" readonly/>
			</div>
		</div>
	</form>
</body>
</html>
