<%@ page contentType="text/html;charset=UTF-8" %>
<%@ page import="org.apache.shiro.web.filter.authc.FormAuthenticationFilter"%>
<%@ page import="org.apache.shiro.authc.ExcessiveAttemptsException"%>
<%@ page import="org.apache.shiro.authc.IncorrectCredentialsException"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<c:set var="ctx" value="${pageContext.request.contextPath}"/>

<html>
<head>
	<title>效能-项目页面</title>
</head>

<body>
	<%-- <a href="${ctx}/efficiency/addPage">新建项目</a>	
	<a href="${ctx}/efficiency/editPage?proId=${project.id}">修改项目</a> --%>
	<form id="loginForm" action="${ctx}/login" method="post" class="form-horizontal">
	</form>
	
	计划时间：<br/>
	<table class="table table-striped table-bordered table-condensed">
		<thead>
		   <tr>
		   		<td>项目ID</td>
		   		<td>项目名称</td>
		   		<td>项目人员</td>
		   		<td>计划工时</td>
		   		<td>实际工时</td>
		   		<td>产出工时</td>
		   		
		   		<td>项目经理</td>
		   		<td>项目状态</td>
		   		<td>操作</td>
		   </tr>
	   	</thead>
	   	<tbody>
		   <c:forEach items="${list}" var="listTemp" varStatus="s" >
				<tr>
			   		<td>${listTemp.proId}</td>
			   		<td>${listTemp.name}</td>
			   		<td>${listTemp.projectPersonnel}</td>
			   		<td>${listTemp.planHoursAll}</td>
			   		
			   		<td>${listTemp.actualHoursAll}</td>
			   		<td>${listTemp.outputHoursAll}</td>
			   		<td>${listTemp.pm}</td>
			   		
			   		<td>${listTemp.state}</td>
			   		<td><a href="cheack?proId=${listTemp.proId}">查看</a></td>
		   		</tr>
		   </c:forEach>
	   	</tbody>
	
	</table>
	
</body>
</html>
