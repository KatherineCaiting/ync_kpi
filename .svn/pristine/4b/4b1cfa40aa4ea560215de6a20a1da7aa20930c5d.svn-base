<%@ page contentType="text/html;charset=UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
</head>
<body>
	<table>
		<tr>
			<td>月度客户满意</td>
		</tr>
		<tr>
			<td>被评方：</td>
			<td>${satisfactionDetailPage.beEvaluatedName}</td>
			<td>时间：</td>
			<td>${satisfactionDetailPage.evaluatedTime}</td>
			<td>项目数</td>
			<td>${satisfactionDetailPage.projectNum}</td>
			<td>部门：</td>
			<td>${satisfactionDetailPage.beEvaluatedDepartmentName}</td>
		</tr>
	</table>
	<h4>项目满意详情</h4>
	<c:forEach var="listProject" items="${satisfactionDetailPage.project}">
		<h4>${listProject.projectName }</h4>
		<table align="center" border="1px;"
			style="border-collapse: collapse; width: 800px;"
			bordercolor="#000000">
			<tr>
				<td>测评方</td>
				<td>部门</td>
				<td>时间</td>
				<td>评分</td>
				<td>评语</td>
			</tr>

			<c:forEach var="listSatisfaction" items="${listProject.list}">
				<tr>
					<td>${listSatisfaction.evaluatedName}</td>
					<td>${listSatisfaction.evaluatedDepartmentName}</td>
					<td>${listSatisfaction.evaluatedTime}</td>
					<td>${listSatisfaction.score}</td>
					<td>${listSatisfaction.comment}</td>
				</tr>
			</c:forEach>
		</table>
	</c:forEach>
</body>
</html>