<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/admin/specialty/addPage</pattern>
		<pattern>/admin/specialty/deletePage*</pattern>		
	</excludes>

	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="admin" page="adminDefault.jsp">
		<pattern>/admin/*</pattern>
	</decorator>
	<decorator name="iframeDefault" page="iframeDefault.jsp">
		<pattern>/efficiency/*</pattern>
	</decorator>
</decorators>
